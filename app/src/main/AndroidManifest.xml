<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.attendencetakerfinal">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AttendenceOfStudent" />
        <activity android:name=".createSubject" />
        <activity
            android:name=".Attendence"
            android:label="@string/title_activity_attendence"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".defaultStudentList" />
        <activity android:name=".reportShow" />
        <activity android:name=".reportTableInfo" />
        <activity android:name=".ExcelInput" />
        <activity android:name=".termtest" />
        <activity
            android:name=".termtestInput"
            android:label="@string/title_activity_termtest_input"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>